package DAY9;
class animal
{
	String color="red";
}
class bird
{
	String color="pink";
}
class dog extends bird
{
	String color="white";
	void display() 
	{
		System.out.println(color);
		System.out.println(super.color);
	}
}
public class Superkeyword {
	public static void main(String[] args) {
		dog obj=new dog();
		obj.display();
		
	}

}
