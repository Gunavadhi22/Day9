package DAY9;

abstract class bike {
    abstract void run();
}

class honda extends bike {
    void run() {
        System.out.println("vehicle");
    }
    void play() {
        System.out.println("playing");
    }
}

public class Interface {
    public static void main(String[] args) {
        bike obj = new honda();
        obj.run();
        honda obj1 = new honda();
        obj1.run();
        obj1.play();
    }
}
